create table tbl_filecontent(
id int primary key,
filename varchar2(20),
filecontent clob);

create table tbl_picture(
id int primary key,
picturename varchar2(20),
picturecontent blob);


select m.empno,m.ename,m.job,m.mgr,m.hiredate,m.sal,m.comm,m.deptno from  
(select rownum r ,empno,ename,job
,mgr,hiredate,sal,comm,deptno from emp where rownum<=10) m where m.r>=6;